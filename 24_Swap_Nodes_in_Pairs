/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) { val = x; }
 * }
 */
class Solution {

    //5ms
    public ListNode swapPairs(ListNode head) {
        ListNode res = new ListNode(0); //dump head;
        res.next = head;
        ListNode index = res;
        ListNode i = head;
        
        while (i != null && i.next != null) {
            ListNode tmp = i.next;
            i.next = i.next.next;
            tmp.next = i;
            index.next = tmp;
            index = index.next.next;
            i = index.next;
        }
        return res.next;
    }
}
